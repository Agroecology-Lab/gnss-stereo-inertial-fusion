<?xml version="1.0" encoding="UTF-8"?> 
<launch> 
    <param name="/use_sim_time" value="true"/>

    <arg name="vocabulary" default="$(find GNSS_SI)/../../../Vocabulary/ORBvoc.txt" doc="Vocabulary for DBoW2."/> 
    <arg name="settings" default="$(find GNSS_SI)/../../Stereo-Inertial/EuRoC_old.yaml" doc="System settings."/> 
    <arg name="map_frame_id" default="map"/>
    <arg name="base_link_frame_id" default="base_link"/>
    <node name="orbslam3_gnss_stereo_inertial" pkg="GNSS_SI" type="GNSS_Stereo_Inertial" output="screen" args="$(arg vocabulary) $(arg settings) true"> 
        <remap from="/camera/left/image_raw" to="/cam0/image_raw"/>
        <remap from="/camera/right/image_raw" to="/cam1/image_raw"/>
        <remap from="/imu" to="/imu0"/>
            <!-- <remap from="/gps/fix" to="/vicon/firefly_sbx/firefly_sbx" />     -->
        <param name="map_frame_id" value="$(arg map_frame_id)"/>
        <param name="base_link_frame_id" value="$(arg base_link_frame_id)"/>
    </node>

    <node pkg="tf2_ros" type="static_transform_publisher" name="start_broadcaster" args="0 0 0 0 0 0 start map"/>
</launch>
